{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build UnderwaterTrees",
      "type": "shell",
      "command": ".\\gradlew.bat",
      "args": ["build"],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "group": "build",
      "problemMatcher": []
    },
    {
      "label": "Clean UnderwaterTrees",
      "type": "shell",
